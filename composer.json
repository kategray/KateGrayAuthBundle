{
    "name": "kategray/authbundle",
    "type": "library",
    "description": "Authentication Bundle for Symfony 2 and 3",
    "keywords": ["auth", "cas"],
    "homepage": "https://github.com/kategray/KateGrayAuthBundle",
    "license": "GPL",
    "authors": [
        {
            "name": "Kate Gray",
            "email": "opensource@codebykate.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "sensio/framework-extra-bundle":  "^3.0",
        "symfony/finder":                 "^2.7|^3.0",
        "symfony/framework-bundle":       "^2.7|^3.0",
        "symfony/security-bundle":        "^2.7|^3.0"
    },
    "require-dev": {
        "sllh/php-cs-fixer-styleci-bridge": "^2.1",
        "symfony/browser-kit":            "^2.7|^3.0",
        "symfony/twig-bundle":            "^2.7|^3.0",
        "symfony/web-profiler-bundle":    "^2.7|^3.0"
    },
    "autoload": {
        "psr-0": { "KateGray\\Auth": "" }
    },
    "target-dir": "kategray/auth"
}
